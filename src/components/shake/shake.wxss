.shake_box{
  margin: 30rpx auto;
  width: 450rpx;
  height: 508rpx;
  position: relative;
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-position: center center;
  background-size: 210rpx 210rpx;
  overflow: hidden;
}

.shake_box_top, .shake_box_bottom{
  position: absolute;
  width: 450rpx;
  height: 254rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 450rpx 508rpx;
}

.shake_box_top{
  top: 0;
}

.shake_box_bottom{
  bottom: 0;
  background-position: 0 -254rpx;
}

.shake_box.anim .shake_box_top{
  animation: shake_top 1.2s linear both;
}

.shake_box.anim .shake_box_bottom{
  animation: shake_bot 1.2s linear both;
}

@keyframes shake_top{
  0%, 100%{
    transform: translateY(0) translateZ(0);
  }
  40%, 60%{
    transform: translateY(-254rpx) translateZ(0);
  }  
}
@keyframes shake_bot{
  0%, 100%{
    transform: translateY(0) translateZ(0);
  }
  40%, 60%{
    transform: translateY(254rpx) translateZ(0);
  }  
}